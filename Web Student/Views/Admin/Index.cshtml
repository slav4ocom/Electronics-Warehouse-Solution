@{
    //var myContext = new CommonModels.StudentDbContext();
    //var students = myContext.UserProfiles;
    var students = ViewBag.StudentProfiles;
}

<h2>Моите ученици</h2>

<br />

<div>
    @{

        {

            <div class="row">
                @foreach (var student in students)
                {
                    <div class="col-md-3">
                     
                        <a href="Homework/MyHomeworks/@(student.Id)">
                            <img src="@PictureProcessing.PictureProcessor.profilePathWeb/@student.Picture" />
                        </a>
                        <br />
                        @student.FullName
                        <br />
                    </div>

                }

            </div>
        }

    }
</div>